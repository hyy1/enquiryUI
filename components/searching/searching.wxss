/* components/searching/searching.wxss */

.search {
  background: #fff;
  overflow: hidden;
  border-bottom: 2rpx solid #e0e0e0;
}

.searching {
  margin: 10rpx 30rpx 12rpx;
  height: 70rpx;
  border-radius: 200rpx;
  background: #f7f7f7;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
}

.img {
  margin-right: 10rpx;
}

.label {
  font-size: 28rpx;
  flex: 1;
  height: 38rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
