/* pages/home/analysisReport/operating/operating.wxss */
.operating {
  background: #ffffff;
  min-height: 970rpx;
  width: 690rpx;
  margin: 0 auto;
  margin-top: 18rpx;
  padding-bottom: 90rpx;
  box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.16);
  border-radius: 6px;
  position: relative;
}
.operating .books {
  width: 91.8rpx;
  height: 90rpx;
  position: absolute;
  bottom: -10rpx;
  right:-12rpx;  
  background-color: #f2f2f2;
  border-radius: 3px;
}
