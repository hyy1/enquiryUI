.operating .img {
  width: 91.8rpx;
  height: 90rpx;
}
.operating .tittle {
  display: flex;
  justify-content: center;
}
.operating .tittle .txt {
  font-size: 14px;
  color: #333333;
  margin-top: 36rpx;
}
.operating .header {
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  margin: 0 52rpx;
  margin-top: 31rpx;
  border-bottom: 1px solid #E0E0E0;
}
.operating .header .head {
  display: flex;
  justify-content: flex-start;
}
.operating .header .content {
  font-size: 14px;
  min-height: 93rpx;
  display: flex;
  align-items: center;
  color: #333333;
  line-height:50rpx;  
}
.operating .header .head .pic image{
  width: 30rpx;
  height: 38rpx;
  float: left;  
}
.operating .header .head .txt {
  margin-left: 13rpx;
  font-size: 14px;
  color: #999999;
}
