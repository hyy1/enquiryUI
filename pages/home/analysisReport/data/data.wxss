/* pages/home/analysisReport/data/data.wxss */

.hidden {
    display: none;
}

view {
    word-break: break-all;
}

.enquiry {
    background-color: #fff;
    padding: 40rpx 30rpx;
    overflow: hidden;
}

.enquiry-tab {
    font-size: 24rpx;
    border-left: 1px solid #e0e0e0;
    border-bottom: 1px solid #e0e0e0;
}

.enquiry-tab .row {
    /* display: flex; */
    justify-content: space-between;
    align-items: center;
    border-top: 1px solid #e0e0e0;
    display: table;
    width: 100%;
}

.enquiry-tab .col {
    text-align: center;
    border-right: 1px solid #e0e0e0;
    /* height: 60rpx; *//* line-height: 60rpx; */
    color: #333;
    height: 60rpx;
    padding: 5rpx 10rpx;
}

.enquiry-tab .txt-width {
    width: 202rpx;
    overflow: hidden;
}

.enquiry-tab .flex1 {
    flex: 1;
    display: table-cell;
    vertical-align: middle;
    width: 25%;
}

.enquiry-tab .title {
    background-color: #f3f7f8;
}

.canvas {
    padding: 30rpx;
}

.canvas .canvas-title {
    text-align: center;
    background: #fff;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 32rpx;
    padding-top: 46rpx;
}

.nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
    color: #333;
    border-bottom: 1px solid #e0e0e0;
    border-top: 1px solid #e0e0e0;
    background-color: #f9fcfc;
}

.nav .flex1 {
    flex: 1;
}

.nav .time {
    width: 40rpx;
    height: 40rpx;
    margin: 0 30rpx;
}

.nav .col {
    height: 70rpx;
    display: flex;
}

.bor-right {
    border-right: 1px solid #e0e0e0;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    align-self: center;
    flex: 1;
}

.nav .active11, .nav .active22 {
    color: #fc7c1f;
    border-bottom: 2px solid #fc7c1f;
}

.nav .active .bor-right {
    font-size: 26rpx;
}

.sjx {
    width: 48rpx;
    height: 24rpx;
    margin: auto;
    overflow: hidden;
}

.sjx .pic {
    width: 48rpx;
    height: 24rpx;
}

/* 空数据 */

.no-data {
    text-align: center;
    font-size: 28rpx;
    color: #ccc;
    margin-top: 212rpx;
}

.no-data .img {
    display: block;
    width: 190rpx;
    height: 178rpx;
    margin: auto;
}

.no-data .txt {
    margin-top: 40rpx;
    display: block;
}

.lab {
    display: flex;
    background: #fff;
    font-size: 24rpx;
    padding-bottom: 30rpx;
    justify-content: center;
    flex-wrap: wrap;
}

.item-lable {
    margin-right: 10rpx;
    display: flex;
}

.lable2 {
    flex-direction: column;
    padding: 20rpx 62rpx;
}

.lable2 .item-lable {
    padding: 10rpx 0;
}

.item-lable-tu {
    width: 25rpx;
    height: 25rpx;
    display: inline-block;
    margin-right: 20rpx;
    align-self: center;
}

.item-lable-txt {
    flex: 1;
}
