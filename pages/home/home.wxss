/* pages/home/home.wxss */

.hidden {
  display: none !important;
}

.home {
  /* max-width: 414px; *//* margin: auto; */
}

.banner {
  width: 100%;
  height: 160rpx;
}

.company {
  background: #fff;
}

.title {
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  background: #fff;
  border-top: 1px solid;
  border-color: #e0e0e0;
  font-size: 32rpx;
  color: #2f3245;
  /* background: -webkit-linear-gradient(left, #55abf6, #7b73fe); *//* -webkit-background-clip: text; *//* -webkit-text-fill-color: transparent; */
}

/* 询盘 */

.enquiry {
  background: #fff;
  /* padding-bottom: 40rpx; */
  margin-bottom: 20rpx;
}

.enquiry .section {
  margin: 0 30rpx;
  margin-top: 7rpx;
  position: relative;
  padding-bottom: 40rpx;
}

.enquiry .section::before {
  content: '';
  display: table;
  clear: both;
}

