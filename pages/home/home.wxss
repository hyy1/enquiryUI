.enquiry .real-setion .navigator {
  border: 1rpx solid #e0e0e0;
  display: flex;
  /* height: 133rpx; */
  /* line-height: 133rpx; */
  align-items: center;
  padding: 44rpx 54rpx;
  box-shadow: 0 4rpx 8rpx 0 rgba(168, 168, 169, 0.31);
  border-radius: 6rpx;
  margin-bottom: 20rpx;
}

.enquiry .real-setion .navigator:last-child {
  margin-bottom: 0;
}

.enquiry .real-setion .icon {
  width: 44rpx;
  height: 44rpx;
  margin-right: 22rpx;
}

.real-setion .left {
  flex: 2;
}

.real-setion .left.spel {
  flex: 3;
}

.real-setion .right {
  flex: 2;
}

.real-setion .left {
  font-size: 28rpx;
  text-align: left;
}

.real-setion .left .t1, .real-setion .left .t2 {
  display: block;
}

.real-setion .left .t1 {
  color: #999;
}

.real-setion .left .t2 {
  color: #151515;
}

.real-setion .left .red {
  font-size: 32rpx;
  color: #cb2b2b;
  width:200rpx;
  word-break:break-all;
}

.real-setion .right {
  text-align: right;
  font-size: 28rpx;
  color: #999;
  /* padding-right: 32rpx; *//* background: url('../../images/home/right@2x.png') no-repeat center right; *//* background-size: 14rpx 28rpx; */
}

.real-setion .right text {
  padding-right: 18rpx;
  width:90rpx;
  word-break:break-all;
}

.real-setion .right .img {
  width: 14rpx;
  height: 28rpx;
  margin-bottom: -3rpx;
}

/* 客户地区 */

.custom-area {
  background: #fff;
}

.custom-area .list-title {
  display: flex;
  background: #f6f6f6;
}

.custom-area .list-subtitle {
  flex: 1;
  text-align: center;
  font-size: 26rpx;
  color: #666;
  padding: 12rpx 0 13rpx;
  border-bottom: 1rpx solid #e0e0e0;
}

.custom-area .list {
  background-color: #f2f2f2;
}

.custom-area .list .list-item {
  display: flex;
  padding: 0 30rpx;
  height: 142rpx;
  align-items: center;
  box-shadow: 0 2rpx 0 0 #e0e0e0;
  margin-bottom: 6rpx;
  background: #fff;
}

.custom-area .list .list-item:last-child {
  margin: 0;
}

.custom-area .list .left {
  flex: 1;
  font-size: 30rpx;
  color: #2f3245;
}

.custom-area .list .left .red {
  font-size: 32rpx;
  color: #cb2b2b;
  width: 255rpx;
  word-break:break-all;
}

.custom-area .list .left text {
  display: block;
}

.custom-area .list .left .v {
  font-size: 24rpx;
  color: #999;
}

.custom-area .list .icon {
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 28rpx;
  color: #55abf6;
  text-align: center;
  margin-right: 18rpx;
  border-radius: 50%;
  border: 1rpx solid #55abf6;
  background: -webkit-linear-gradient(left, #55abf6, #7b73fe);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.custom-area .list .right {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.custom-area .list .right .img {
  width: 14rpx;
  height: 28rpx;
  margin-bottom: -3rpx;
}

.progress-bar {
  border-radius: 50%;
  width: 93.6rpx;
  height: 93.6rpx;
  background: #d8d8d8;
  position: relative;
  margin-right: 23.2rpx;
  font-size: 24rpx;
  color: #6495f9;
  background-image: linear-gradient(90deg, transparent 60%, #d8d8d8 60%),
                      linear-gradient(90deg, #7b73fe 0%, #55abf6 0%);
  /* background-image: linear-gradient(0deg, #7b73fe 0%, #55abf6 100%);  */
}

.progress-bar .overlay {
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
}

.custom-area .ring {
  width: 93.6rpx;
  height: 93.6rpx;
  margin-right: 23.2rpx;
}

.custom-area .list .t1, .custom-area .list .t2 {
  font-size: 24rpx;
  color: #333;
  display: block;
  text-align: right;
}

.custom-area .list .t1 {
}

.custom-area .list .t2 {
  font-size: 28rpx;
  color: #f44;
}

/* 图表 */

.container {
  border-top: 1rpx solid #e0e0e0;
}

.canvas {
  padding-top: 20rpx;
  border-bottom: 1rpx solid #e0e0e0;
}

canvas {
  width: 100%;
  height: 446rpx;
  background: #fff;
}

